<h1>Products</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th>Supplier</th>
    <th></th>
    <th>Description</th>
    <th></th>
    <th>Stock</th>
    <th></th>
    <th>Cost</th>
    <th></th>
    <th>Price</th>
    </tr>
    <% for product in @products %>
    <tr>
      <td><%= product.name %></td>
      <td>|</td>
      <td> <%= product.supplier_id %></td>
      <td>|</td>
      <td> <%= product.description %></td>
      <td>|</td>
      <td> <%= product.stock_quantity %></td>
      <td>|</td>
      <td> <%= product.buying_cost %></td>
      <td>|</td>
      <td> <%= product.selling_price %></td>
    </tr>
  <% end %>
</table>
